{
    "configurations": [
        {
            "name": "cross_amiga",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/amiga/m68k-amigaos/ndk-include/",
                "/opt/amiga/m68k-amigaos/include/",
                "/opt/amiga/m68k-amigaos/vbcc/include"
            ],
            "defines": [],
            "compilerPath": "/opt/amiga/bin/m68k-amigaos-g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}"
        },
        {
            "name": "cross_morphos",
            "includePath": [
                "${workspaceFolder}/**",
                "/gg/include/",
                "/gg/os-include/",
                "/gg/includestd/"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/ppc-morphos-g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "compilerPathInCppPropertiesJson": "/usr/bin/ppc-morphos-g++",
            "mergeConfigurations": false,
            "recursiveIncludes": {},
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}